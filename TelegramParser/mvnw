# ----------------------------------------------------------------------------
# Apache Maven Wrapper startup batch script, version 3.3.2
#
# Optional ENV vars
# -----------------
#***REMOVED*** JAVA_HOME - location of a JDK home dir, required when download maven via java source
#***REMOVED*** MVNW_REPOURL - repo url base for downloading maven distribution
#***REMOVED*** MVNW_USERNAME/MVNW_PASSWORD - user and password for downloading maven
#***REMOVED*** MVNW_VERBOSE - true: enable verbose log; debug: trace the mvnw script; others: silence the output
# ----------------------------------------------------------------------------

set -euf
[ "${MVNW_VERBOSE-}" != debug ] || set -x

# OS specific support.
native_path() { printf %s\\n "$1"; }
case "$(uname)" in
CYGWIN* | MINGW*)
***REMOVED***[ -z "${JAVA_HOME-}" ] || JAVA_HOME="$(cygpath --unix "$JAVA_HOME")"
***REMOVED***native_path() { cygpath --path --windows "$1"; }
***REMOVED***;;
esac

# set JAVACMD and JAVACCMD
set_java_home() {
***REMOVED***# For Cygwin and MinGW, ensure paths are in Unix format before anything is touched
***REMOVED***if [ -n "${JAVA_HOME-}" ]; then
***REMOVED******REMOVED***if [ -x "$JAVA_HOME/jre/sh/java" ]; then
***REMOVED******REMOVED******REMOVED***# IBM's JDK on AIX uses strange locations for the executables
***REMOVED******REMOVED******REMOVED***JAVACMD="$JAVA_HOME/jre/sh/java"
***REMOVED******REMOVED******REMOVED***JAVACCMD="$JAVA_HOME/jre/sh/javac"
***REMOVED******REMOVED***else
***REMOVED******REMOVED******REMOVED***JAVACMD="$JAVA_HOME/bin/java"
***REMOVED******REMOVED******REMOVED***JAVACCMD="$JAVA_HOME/bin/javac"

***REMOVED******REMOVED******REMOVED***if [ ! -x "$JAVACMD" ] || [ ! -x "$JAVACCMD" ]; then
***REMOVED******REMOVED******REMOVED******REMOVED***echo "The JAVA_HOME environment variable is not defined correctly, so mvnw cannot run." >&2
***REMOVED******REMOVED******REMOVED******REMOVED***echo "JAVA_HOME is set to \"$JAVA_HOME\", but \"\$JAVA_HOME/bin/java\" or \"\$JAVA_HOME/bin/javac\" does not exist." >&2
***REMOVED******REMOVED******REMOVED******REMOVED***return 1
***REMOVED******REMOVED******REMOVED***fi
***REMOVED******REMOVED***fi
***REMOVED***else
***REMOVED******REMOVED***JAVACMD="$(
***REMOVED******REMOVED******REMOVED***'set' +e
***REMOVED******REMOVED******REMOVED***'unset' -f command 2>/dev/null
***REMOVED******REMOVED******REMOVED***'command' -v java
***REMOVED******REMOVED***)" || :
***REMOVED******REMOVED***JAVACCMD="$(
***REMOVED******REMOVED******REMOVED***'set' +e
***REMOVED******REMOVED******REMOVED***'unset' -f command 2>/dev/null
***REMOVED******REMOVED******REMOVED***'command' -v javac
***REMOVED******REMOVED***)" || :

***REMOVED******REMOVED***if [ ! -x "${JAVACMD-}" ] || [ ! -x "${JAVACCMD-}" ]; then
***REMOVED******REMOVED******REMOVED***echo "The java/javac command does not exist in PATH nor is JAVA_HOME set, so mvnw cannot run." >&2
***REMOVED******REMOVED******REMOVED***return 1
***REMOVED******REMOVED***fi
***REMOVED***fi
}

# hash string like Java String::hashCode
hash_string() {
***REMOVED***str="${1:-}" h=0
***REMOVED***while [ -n "$str" ]; do
***REMOVED******REMOVED***char="${str%"${str#?}"}"
***REMOVED******REMOVED***h=$(((h * 31 + $(LC_CTYPE=C printf %d "'$char")) % 4294967296))
***REMOVED******REMOVED***str="${str#?}"
***REMOVED***done
***REMOVED***printf %x\\n $h
}

verbose() { :; }
[ "${MVNW_VERBOSE-}" != true ] || verbose() { printf %s\\n "${1-}"; }

die() {
***REMOVED***printf %s\\n "$1" >&2
***REMOVED***exit 1
}

trim() {
***REMOVED***# MWRAPPER-139:
***REMOVED***#***REMOVED*** Trims trailing and leading whitespace, carriage returns, tabs, and linefeeds.
***REMOVED***#***REMOVED*** Needed for removing poorly interpreted newline sequences when running in more
***REMOVED***#***REMOVED*** exotic environments such as mingw bash on Windows.
***REMOVED***printf "%s" "${1}" | tr -d '[:space:]'
}

# parse distributionUrl and optional distributionSha256Sum, requires .mvn/wrapper/maven-wrapper.properties
while IFS="=" read -r key value; do
***REMOVED***case "${key-}" in
***REMOVED***distributionUrl) distributionUrl=$(trim "${value-}") ;;
***REMOVED***distributionSha256Sum) distributionSha256Sum=$(trim "${value-}") ;;
***REMOVED***esac
done <"${0%/*}/.mvn/wrapper/maven-wrapper.properties"
[ -n "${distributionUrl-}" ] || die "cannot read distributionUrl property in ${0%/*}/.mvn/wrapper/maven-wrapper.properties"

case "${distributionUrl##*/}" in
maven-mvnd-*bin.*)
***REMOVED***MVN_CMD=mvnd.sh _MVNW_REPO_PATTERN=/maven/mvnd/
***REMOVED***case "${PROCESSOR_ARCHITECTURE-}${PROCESSOR_ARCHITEW6432-}:$(uname -a)" in
***REMOVED****AMD64:CYGWIN* | *AMD64:MINGW*) distributionPlatform=windows-amd64 ;;
***REMOVED***:Darwin*x86_64) distributionPlatform=darwin-amd64 ;;
***REMOVED***:Darwin*arm64) distributionPlatform=darwin-aarch64 ;;
***REMOVED***:Linux*x86_64*) distributionPlatform=linux-amd64 ;;
***REMOVED****)
***REMOVED******REMOVED***echo "Cannot detect native platform for mvnd on $(uname)-$(uname -m), use pure java version" >&2
***REMOVED******REMOVED***distributionPlatform=linux-amd64
***REMOVED******REMOVED***;;
***REMOVED***esac
***REMOVED***distributionUrl="${distributionUrl%-bin.*}-$distributionPlatform.zip"
***REMOVED***;;
maven-mvnd-*) MVN_CMD=mvnd.sh _MVNW_REPO_PATTERN=/maven/mvnd/ ;;
*) MVN_CMD="mvn${0##*/mvnw}" _MVNW_REPO_PATTERN=/org/apache/maven/ ;;
esac

# apply MVNW_REPOURL and calculate MAVEN_HOME
# maven home pattern: ~/.m2/wrapper/dists/{apache-maven-<version>,maven-mvnd-<version>-<platform>}/<hash>
[ -z "${MVNW_REPOURL-}" ] || distributionUrl="$MVNW_REPOURL$_MVNW_REPO_PATTERN${distributionUrl#*"$_MVNW_REPO_PATTERN"}"
distributionUrlName="${distributionUrl##*/}"
distributionUrlNameMain="${distributionUrlName%.*}"
distributionUrlNameMain="${distributionUrlNameMain%-bin}"
MAVEN_USER_HOME="${MAVEN_USER_HOME:-${HOME}/.m2}"
MAVEN_HOME="${MAVEN_USER_HOME}/wrapper/dists/${distributionUrlNameMain-}/$(hash_string "$distributionUrl")"

exec_maven() {
***REMOVED***unset MVNW_VERBOSE MVNW_USERNAME MVNW_PASSWORD MVNW_REPOURL || :
***REMOVED***exec "$MAVEN_HOME/bin/$MVN_CMD" "$@" || die "cannot exec $MAVEN_HOME/bin/$MVN_CMD"
}

if [ -d "$MAVEN_HOME" ]; then
***REMOVED***verbose "found existing MAVEN_HOME at $MAVEN_HOME"
***REMOVED***exec_maven "$@"
fi

case "${distributionUrl-}" in
*?-bin.zip | *?maven-mvnd-?*-?*.zip) ;;
*) die "distributionUrl is not valid, must match *-bin.zip or maven-mvnd-*.zip, but found '${distributionUrl-}'" ;;
esac

# prepare tmp dir
if TMP_DOWNLOAD_DIR="$(mktemp -d)" && [ -d "$TMP_DOWNLOAD_DIR" ]; then
***REMOVED***clean() { rm -rf -- "$TMP_DOWNLOAD_DIR"; }
***REMOVED***trap clean HUP INT TERM EXIT
else
***REMOVED***die "cannot create temp dir"
fi

mkdir -p -- "${MAVEN_HOME%/*}"

# Download and Install Apache Maven
verbose "Couldn't find MAVEN_HOME, downloading and installing it ..."
verbose "Downloading from: $distributionUrl"
verbose "Downloading to: $TMP_DOWNLOAD_DIR/$distributionUrlName"

# select .zip or .tar.gz
if ! command -v unzip >/dev/null; then
***REMOVED***distributionUrl="${distributionUrl%.zip}.tar.gz"
***REMOVED***distributionUrlName="${distributionUrl##*/}"
fi

# verbose opt
__MVNW_QUIET_WGET=--quiet __MVNW_QUIET_CURL=--silent __MVNW_QUIET_UNZIP=-q __MVNW_QUIET_TAR=''
[ "${MVNW_VERBOSE-}" != true ] || __MVNW_QUIET_WGET='' __MVNW_QUIET_CURL='' __MVNW_QUIET_UNZIP='' __MVNW_QUIET_TAR=v

# normalize http auth
case "${MVNW_PASSWORD:+has-password}" in
'') MVNW_USERNAME='' MVNW_PASSWORD='' ;;
has-password) [ -n "${MVNW_USERNAME-}" ] || MVNW_USERNAME='' MVNW_PASSWORD='' ;;
esac

if [ -z "${MVNW_USERNAME-}" ] && command -v wget >/dev/null; then
***REMOVED***verbose "Found wget ... using wget"
***REMOVED***wget ${__MVNW_QUIET_WGET:+"$__MVNW_QUIET_WGET"} "$distributionUrl" -O "$TMP_DOWNLOAD_DIR/$distributionUrlName" || die "wget: Failed to fetch $distributionUrl"
elif [ -z "${MVNW_USERNAME-}" ] && command -v curl >/dev/null; then
***REMOVED***verbose "Found curl ... using curl"
***REMOVED***curl ${__MVNW_QUIET_CURL:+"$__MVNW_QUIET_CURL"} -f -L -o "$TMP_DOWNLOAD_DIR/$distributionUrlName" "$distributionUrl" || die "curl: Failed to fetch $distributionUrl"
elif set_java_home; then
***REMOVED***verbose "Falling back to use Java to download"
***REMOVED***javaSource="$TMP_DOWNLOAD_DIR/Downloader.java"
***REMOVED***targetZip="$TMP_DOWNLOAD_DIR/$distributionUrlName"
***REMOVED***cat >"$javaSource" <<-END
	public class Downloader extends java.net.Authenticator
	{
	***REMOVED***protected java.net.PasswordAuthentication getPasswordAuthentication()
	***REMOVED***{
	***REMOVED******REMOVED***return new java.net.PasswordAuthentication( System.getenv( "MVNW_USERNAME" ), System.getenv( "MVNW_PASSWORD" ).toCharArray() );
	***REMOVED***}
	***REMOVED***public static void main( String[] args ) throws Exception
	***REMOVED***{
	***REMOVED******REMOVED***setDefault( new Downloader() );
	***REMOVED******REMOVED***java.nio.file.Files.copy( java.net.URI.create( args[0] ).toURL().openStream(), java.nio.file.Paths.get( args[1] ).toAbsolutePath().normalize() );
	***REMOVED***}
	}
	END
***REMOVED***# For Cygwin/MinGW, switch paths to Windows format before running javac and java
***REMOVED***verbose " - Compiling Downloader.java ..."
***REMOVED***"$(native_path "$JAVACCMD")" "$(native_path "$javaSource")" || die "Failed to compile Downloader.java"
***REMOVED***verbose " - Running Downloader.java ..."
***REMOVED***"$(native_path "$JAVACMD")" -cp "$(native_path "$TMP_DOWNLOAD_DIR")" Downloader "$distributionUrl" "$(native_path "$targetZip")"
fi

# If specified, validate the SHA-256 sum of the Maven distribution zip file
if [ -n "${distributionSha256Sum-}" ]; then
***REMOVED***distributionSha256Result=false
***REMOVED***if [ "$MVN_CMD" = mvnd.sh ]; then
***REMOVED******REMOVED***echo "Checksum validation is not supported for maven-mvnd." >&2
***REMOVED******REMOVED***echo "Please disable validation by removing 'distributionSha256Sum' from your maven-wrapper.properties." >&2
***REMOVED******REMOVED***exit 1
***REMOVED***elif command -v sha256sum >/dev/null; then
***REMOVED******REMOVED***if echo "$distributionSha256Sum***REMOVED***$TMP_DOWNLOAD_DIR/$distributionUrlName" | sha256sum -c >/dev/null 2>&1; then
***REMOVED******REMOVED******REMOVED***distributionSha256Result=true
***REMOVED******REMOVED***fi
***REMOVED***elif command -v shasum >/dev/null; then
***REMOVED******REMOVED***if echo "$distributionSha256Sum***REMOVED***$TMP_DOWNLOAD_DIR/$distributionUrlName" | shasum -a 256 -c >/dev/null 2>&1; then
***REMOVED******REMOVED******REMOVED***distributionSha256Result=true
***REMOVED******REMOVED***fi
***REMOVED***else
***REMOVED******REMOVED***echo "Checksum validation was requested but neither 'sha256sum' or 'shasum' are available." >&2
***REMOVED******REMOVED***echo "Please install either command, or disable validation by removing 'distributionSha256Sum' from your maven-wrapper.properties." >&2
***REMOVED******REMOVED***exit 1
***REMOVED***fi
***REMOVED***if [ $distributionSha256Result = false ]; then
***REMOVED******REMOVED***echo "Error: Failed to validate Maven distribution SHA-256, your Maven distribution might be compromised." >&2
***REMOVED******REMOVED***echo "If you updated your Maven version, you need to update the specified distributionSha256Sum property." >&2
***REMOVED******REMOVED***exit 1
***REMOVED***fi
fi

# unzip and move
if command -v unzip >/dev/null; then
***REMOVED***unzip ${__MVNW_QUIET_UNZIP:+"$__MVNW_QUIET_UNZIP"} "$TMP_DOWNLOAD_DIR/$distributionUrlName" -d "$TMP_DOWNLOAD_DIR" || die "failed to unzip"
else
***REMOVED***tar xzf${__MVNW_QUIET_TAR:+"$__MVNW_QUIET_TAR"} "$TMP_DOWNLOAD_DIR/$distributionUrlName" -C "$TMP_DOWNLOAD_DIR" || die "failed to untar"
fi
printf %s\\n "$distributionUrl" >"$TMP_DOWNLOAD_DIR/$distributionUrlNameMain/mvnw.url"
mv -- "$TMP_DOWNLOAD_DIR/$distributionUrlNameMain" "$MAVEN_HOME" || [ -d "$MAVEN_HOME" ] || die "fail to move MAVEN_HOME"

clean || :
exec_maven "$@"
